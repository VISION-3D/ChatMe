{
  "version": 2,
  "name": "chatme-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://api.chatme-app.com",
    "REACT_APP_SOCKET_URL": "wss://chatme-socket.com"
  }
}