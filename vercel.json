{
  "version": 2,
  "name": "chatme",
  "buildCommand": "cd frontend && CI=false npx react-scripts build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && npm install --unsafe-perm",
  "framework": "create-react-app",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}